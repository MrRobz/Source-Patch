{
  "manifest_version": 3,
  "name": "Source Patch",
  "version": "0.0.1",
  "description": "An extension for your browser that makes editing the source code of your website easy.",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [],
  "permissions": ["activeTab", "unlimitedStorage"]
}
